@model LogUser
<h2>Login</h2>
<div class="text-center">
    <div class="row">
        <div class="col">
            <h3>Register</h3>
            <form action="/Login" method="post">
            <div class="form-group">
                <label for="LogEmail">Email</label>
                <input asp-for="LogEmail" class="form-control" value="calebkoehn99@outlook.com">
                <span asp-validation-for="LogEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label for="LogPassword">Password</label>
                <input asp-for="LogPassword" class="form-control" value="password">
                <span asp-validation-for="LogPassword" class="text-danger"></span>
            </div>
            <input type="submit" name="Submit" class="btn btn-primary">
            </form>
        </div>
    </div>
</div>