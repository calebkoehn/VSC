@model Chef
<div class="text-right">
    <a href="/newDish" class="btn btn-secondary">Add a Dish</a>
</div>

<div class="text-center">
    <h1>Yum, take a look at our tasty dishes!</h1>
    <table class="table table-striped">
        <tr>
            <th>Name</th>
            <th>Chef</th>
            <th>Tastiness</th>
            <th>Calories</th>
        </tr>
        @{
            foreach(Dish i in ViewBag.allDishes)
            {
                <tr>
                    <td>@i.Name</td>
                    <td>@i.Creator.FirstName @i.Creator.LastName</td>
                    <td>@i.Tastiness</td>
                    <td>@i.Calories</td>
                </tr>
            }
        }
    </table>
</div>