@model Dish
<div class="text-right">
        <a href="/" class="btn btn-primary mt-5 float-end">Home</a>
</div>
    <div class="text-center">
        <h1>@Model.Name</h1>
        <h3>@Model.Chef</h3>
    </div>

    <div class="container">
        <h4>@Model.Description</h4>
        <h4>Calories:@Model.Calories</h4>
        <h4>Tastiness:@Model.Tastiness</h4>
    </div>

<a href="/delete/@Model.DishId" class="btn btn-danger mt-5 float-start">Delete</a>
<a href="/edit/@Model.DishId" class="btn btn-primary mt-5 float-end">Edit</a>